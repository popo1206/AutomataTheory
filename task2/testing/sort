value sort(pointer A,value N)
{
    value i=0;
    value j=N-2;
    value c;
        while (i<=N-1){
            while  (j>=i){
                notzero? (*(A+j)>=*(A+j+1)){
                    c=*(A+j);
                    *(A+j)=*(A+j+1);
                    *(A+j+1)=c;
                    }
                j=j-1;
            }
            i=i+1;
        }
    return 1;
}

value main()
{
    value n;
    array of value A;
	A[0]=1;
	A[1]=5;
    n=sort(&A[0],sizeof(A));
    return 1;
}
